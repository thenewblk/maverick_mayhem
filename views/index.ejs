  <body>
    <div class="nav-container">
      <nav class="nav--primary" role="navigation">
        <a href="/" class="nav__logo">
          <img src="img/icon-bull.svg" alt="UNO Maverics bull icon">
        </a>
        <ul class="nav__menu nav__menu-disabled">
          <li class="nav__item"><a href="">Hockey</a></li>
          <li class="nav__item"><a href="">New Arena</a></li>
          <li class="nav__item"><a href="">Join</a></li>
        </ul>
      </nav>
      <span class="nav__menu-icon">Menu</span>
      <div class="sport-type-avatar"></div>
    </div>
    <header role="banner">
      <div class="banner">
        <h1 class="page-headline">Mavericks take win over deluth 4-1</h1>
      </div>
    </header>
    <div id="main" role="main">
      <div class="row">
        <div class="container">
          <div class="video-container">
            <div class="video-wrapper">
              <iframe width="853" height="480" src="//www.youtube.com/embed/Mspf3phvhN4?rel=0&amp;showinfo=0" frameborder="0" allowfullscreen></iframe>
            </div>
          </div>
          <div class="sport">
            <div class="game-content">
              <table class="sport__scoreboard sport__scoreboard-hockey">
                <thead>
                  <tr class="scoreboard__header">
                    <th class="team-name">&nbsp;</th>
                    <th>1st</th>
                    <th>2nd</th>
                    <th>3rd</th>
                    <th class="score-total">Total</th>
                  </tr>
                </thead>
                <tbody>
                  <tr class="team visitor">
                    <td class="team-name">Deluth</td>
                    <td class="score">1</td>
                    <td class="score">0</td>
                    <td class="score">0</td>
                    <td class="score">1</td>
                  </tr>
                  <tr class="team away">
                    <td class="team-name">Omaha</td>
                    <td class="score">2</td>
                    <td class="score">1</td>
                    <td class="score">1</td>
                    <td class="score score-winner">4</td>
                  </tr>
                </tbody>
                <tfoot>
                  <tr>
                    <td colspan="5"><a href="#">view stats</a></td>
                  </tr>
                </tfoot>
              </table>
            </div>
            <div class="upcoming-game">
              <h3><span>Next Game: </span>Fri, Nov 28th 2014</h3>
              <p>vs North Dakota @ Grand Forks, N.D</p>
            </div>
            <a href="" class="btn btn--red">Get Tickets</a>
          </div>
        </div>
      </div>
      <div class="row ticket-listing">
        <div class="container">
          <h2>Listed tickets below</h2>
        </div>
      </div>
      <div class="row">
        <div class="container">
          <div class="news">
            <ul class="news__list">
              <li class="news__list-item">
                <a href="">
                  <h4 class="news__title"><span>Newcomers spark Mavericks' surprising start<em class="news__byline">by OWH</em></span></h4>
                </a>
                <img src="img/news-item.jpg" alt="">
              </li>
              <li class="news__list-item">
                <a href="">
                  <h4 class="news__title"><span>Newcomers spark Mavericks' surprising start<em class="news__byline">by OWH</em><span></h4>
                </a>
                <img src="img/news-item.jpg" alt="">
              </li>
            </ul>
          </div>
          <div class="featured-photo">
            <img src="http://placehold.it/800x400" alt="">
          </div>
        </div>
      </div>
      <div class="row">
        <div class="container">
          <ul class="photo-list">
            <li class="photo-list__item"><img src="http://placehold.it/260x260" alt="" class="photo"></li>
            <li class="photo-list__item"><img src="http://placehold.it/260x260" alt="" class="photo"></li>
            <li class="photo-list__item"><img src="http://placehold.it/260x260" alt="" class="photo"></li>
            <li class="photo-list__item"><img src="http://placehold.it/260x260" alt="" class="photo"></li>
            <li class="photo-list__item"><img src="http://placehold.it/260x260" alt="" class="photo"></li>
          </ul>
        </div>
      </div>
    </div>
    <footer>
      <div class="container">
        <p>Copyright &copy; 2015</p>
      </div>
